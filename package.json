{
  "name": "cf-sk-effect-template",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "preview": "turbo run preview",
    "deploy": "turbo run deploy",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,md,svelte}\"",
    "check": "turbo run check",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
