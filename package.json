    "node": ">=24"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "overrides": {
      "vite": "npm:rolldown-vite@latest"
    }
  },
  "dependencies": {
    "effect": "catalog:"
  }
